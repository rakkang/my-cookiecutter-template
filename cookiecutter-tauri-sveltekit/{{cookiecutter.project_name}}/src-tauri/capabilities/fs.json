{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "fs",
  "description": "File system capability",
  "windows": ["main"],
  "permissions": [
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-exists",
    "fs:allow-copy-file",
    "fs:allow-rename",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}

